<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

 

<hibernate-mapping>
		<class name="com.test.models.Answers" table="ANSWERS">
		<id name="answersSid" column="ANSWERS_SID">
			<generator class="native" />
		</id>
		<property name="answerValue" column="ANSWER_VALUE" />
	</class>
	
</hibernate-mapping>